{
  "name": "carvis",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build:client": "node ./tools/scripts/build-client.js",
    "dev:build:client": "node ./tools/scripts/dev-client.js",
    "build:server": "babel ./src/server -d ./dist/server --presets es2015,stage-2,es2016 --copy-files",
    "test": "mocha --compilers js:babel-core/register test/index.js",
    "start:src": "nodemon ./src/server/index.js --exec babel-node --presets es2015,stage-2",
    "start:dist": "npm run build:server --watch; npm run dev:build:client & nodemon ./dist/server/index.js",
    "build:src": "npm run build:client; npm run build:server",
    "build": "npm run build:src; docker build -t complex-joins/carvis .",
    "start": "node ./dist/server/index.js"
  },
  "eslintConfig": {
    "extends": "./tools/eslint.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/complex-joins/carvis.git"
  },
  "author": "Alex Stark",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/complex-joins/carvis/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-stage-2": "^6.13.0",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "eslint": "^3.2.2",
    "eslint-plugin-flowtype": "^2.4.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.0.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "opn": "^4.0.2",
    "sass-loader": "^4.0.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "express-qs-parser": "0.0.1",
    "express-session": "^1.14.0",
    "file-loader": "^0.9.0",
    "grant-express": "^3.6.3",
    "postcss": "^5.1.2",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "request": "^2.74.0",
    "webpack": "^1.13.1"
  },
  "homepage": "https://github.com/complex-joins/carvis#readme"
}
